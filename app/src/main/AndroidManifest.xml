<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hsyen1.symptomsmanagingapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SymptomsListActivity"
            android:label="@string/title_activity_symptoms"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".SymptomsMainActivity" />
        <activity android:name=".DateAndTimeActivity" />
        <activity
            android:name=".SymptomsLogListActivity"
            android:label="@string/title_activity_symptoms_log"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".SymptomsLogActivity" />
        <activity android:name=".QoLifeActivity"></activity>
    </application>

</manifest>